  "scripts": {
    "start-js": "node scripts/start.js",
    "start": "npm-run-all -p start-js",
    "start:mock": "cross-env REACT_APP_MOCK=true npm-run-all -p start-js",
    "build-js": "node scripts/build.js",
    "postbuild": "react-snap",
    "build": "npm-run-all build-js",
    "build:product": "cross-env GENERATE_SOURCEMAP=false npm-run-all build-js && cd build && cp -R . ../../gen-server/app/public/",
    "deploy": "node config/deploy.js",
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "*.less": [
      "stylelint --fix",
      "prettier --parser --single-quote less --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ]
    ]
  }
}
